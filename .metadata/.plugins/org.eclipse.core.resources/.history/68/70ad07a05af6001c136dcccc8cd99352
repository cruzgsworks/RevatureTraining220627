package com.revature;

/**
 * @author Gerard Cruz
 *
 */
public class Main {

	/**
	 * @param args
	 */
	
	private String helloWorldText = "";
	
	public Main() {
		helloWorldText = "Hello World";
	}
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Main helloWorldObj = new Main();
		helloWorldObj.printHelloWorld(helloWorldObj.helloWorldText);
	}
	
	public void printHelloWorld(String arg1) {
		System.out.print(arg1);
	}

}
